int tmp;
    while (pos->value != end)
    {
      if (pos->first_child != NULL)
      {
        if (pos->next_sibling != NULL)
        {
          tmp = pos->value;
        }
        pos = pos->first_child;
      } else {
        if (pos->value == end)
        {
          break;
        }
        pos = search_pos(t,tmp);
        pos = pos->next_sibling;
      }
      
}